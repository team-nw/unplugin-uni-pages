{
  "name": "@team-nw/unplugin-uni-pages",
  "type": "module",
  "version": "0.2.3",
  "packageManager": "pnpm@10.13.1",
  "description": "",
  "license": "MIT",
  "homepage": "https://github.com/team-nw/unplugin-uni-pages#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/team-nw/unplugin-uni-pages.git"
  },
  "bugs": {
    "url": "https://github.com/team-nw/unplugin-uni-pages/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "transform"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./runtime": {
      "import": "./dist/runtime.js",
      "require": "./dist/runtime.cjs"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist",
    "client.d.ts",
    "virtual.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "lint": "eslint .",
    "play": "npm -C playground run dev:mp-weixin",
    "prepublishOnly": "npm run build",
    "release": "bumpp && pnpm publish",
    "start": "tsx src/index.ts",
    "test": "vitest"
  },
  "peerDependencies": {
    "@farmfe/core": ">=1",
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "esbuild": "*",
    "rollup": "^3",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/utils": "^9.2.0",
    "@babel/generator": "^7.28.0",
    "@babel/types": "^7.28.2",
    "@vue/compiler-sfc": "3.5.6",
    "ast-kit": "^1.4.3",
    "chokidar": "3.6.0",
    "debug": "^4.4.1",
    "magic-string-ast": "^0.7.1",
    "typescript": "^5.8.3",
    "unconfig": "^7.3.2",
    "unplugin": "^2.3.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.19.0",
    "@nuxt/kit": "^3.18.0",
    "@nuxt/schema": "^3.18.0",
    "@team-nw/unplugin-uni-pages": "workspace:*",
    "@types/babel__generator": "^7.27.0",
    "@types/node": "^22.16.5",
    "bumpp": "^10.2.1",
    "eslint": "^9.32.0",
    "nodemon": "^3.1.10",
    "rollup": "^4.46.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.4",
    "webpack": "^5.101.0"
  }
}
