{
  "name": "@team-nw/unplugin-uni-pages",
  "type": "module",
  "version": "0.1.3",
  "packageManager": "pnpm@10.3.0",
  "description": "",
  "license": "MIT",
  "homepage": "https://github.com/team-nw/unplugin-uni-pages#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/team-nw/unplugin-uni-pages.git"
  },
  "bugs": {
    "url": "https://github.com/team-nw/unplugin-uni-pages/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "transform"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./runtime": {
      "import": "./dist/runtime.js",
      "require": "./dist/runtime.cjs"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "lint": "eslint .",
    "play": "npm -C playground run dev:mp-weixin",
    "prepublishOnly": "npm run build",
    "release": "bumpp && pnpm publish",
    "start": "tsx src/index.ts",
    "test": "vitest"
  },
  "peerDependencies": {
    "@farmfe/core": ">=1",
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "esbuild": "*",
    "rollup": "^3",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "@babel/generator": "^7.26.9",
    "@babel/types": "^7.26.9",
    "@vue-macros/common": "^1.16.1",
    "ast-kit": "^1.4.0",
    "chokidar": "3.6.0",
    "debug": "^4.4.0",
    "typescript": "^5.7.3",
    "unconfig": "^7.1.0",
    "unplugin": "^2.1.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.2.0",
    "@nuxt/kit": "^3.15.4",
    "@nuxt/schema": "^3.15.4",
    "@types/babel__generator": "^7.6.8",
    "@types/node": "^22.13.1",
    "bumpp": "^10.0.2",
    "eslint": "^9.20.0",
    "nodemon": "^3.1.9",
    "rollup": "^4.34.6",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "vite": "^6.1.0",
    "vitest": "^3.0.5",
    "webpack": "^5.97.1"
  }
}
